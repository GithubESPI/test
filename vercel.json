{
  "functions": {
    "app/api/**/*": {
      "maxDuration": 300
    },
    "app/api/pdf/route.ts": {
      "maxDuration": 900
    }
  },
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}
